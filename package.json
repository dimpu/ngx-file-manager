{
  "name": "ngx-file-manager",
  "version": "7.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run lib.build && concurrently 'npm:lib.build.watch' 'npm:demo.serve' 'npm:server.start'",
    "lib.build": "ng build @dimpu/ngx-file-manager",
    "lib.build.watch": "ng build @dimpu/ngx-file-manager --watch",
    "lib.test": "ng test @dimpu/ngx-file-manager",
    "lib.lint": "ng lint @dimpu/ngx-file-manager",
    "demo.serve": "ng serve ngx-file-manager-demo",
    "demo.build": "ng build ngx-file-manager-demo",
    "demo.test": "ng test ngx-file-manager-demo",
    "demo.lint": "ng lint ngx-file-manager-demo",
    "demo.deploy": "npm run demo.build -- --base-href='/ngx-file-manager/' && gh-pages -d dist/ngx-file-manager-demo/",
    "server.start": "cd projects/file-manager-server && npm start",
    "npm.publish": "npm version patch && cd projects/dimpu/ngx-file-manager && npm version patch && cd ../../../ && npm run lib.build && cp ./README.md dist/dimpu/ngx-file-manager && cd dist/dimpu/ngx-file-manager  && npm config set scope dimpu && npm publish --access=public"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/flex-layout": "^7.0.0-beta.19",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "is-image": "^2.0.0",
    "rxjs": "~6.3.3",
    "uuid": "^3.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.0.1",
    "gh-pages": "^2.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.1"
  }
}
